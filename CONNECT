                particle.Speed = NumberRange.new(0.003990, 0.003990)
                particle.Brightness = 1
                particle.Size = NumberSequence.new({NumberSequenceKeypoint.new(0.000000, 0.000000), NumberSequenceKeypoint.new(0.100000, 1.239665), NumberSequenceKeypoint.new(0.200000, 2.169628), NumberSequenceKeypoint.new(0.300000, 2.865901), NumberSequenceKeypoint.new(0.400000, 3.389447), NumberSequenceKeypoint.new(0.500000, 3.791799), NumberSequenceKeypoint.new(0.600000, 4.118867), NumberSequenceKeypoint.new(0.700000, 4.413821), NumberSequenceKeypoint.new(0.800000, 4.719571), NumberSequenceKeypoint.new(0.900000, 5.081281), NumberSequenceKeypoint.new(1.000000, 5.549332), NumberSequenceKeypoint.new(1.000000, 5.549332)})
                particle.Enabled = false
                particle.Acceleration = Vector3.new(0.000000, 0.000000, 0.000000)
                particle.RotSpeed = NumberRange.new(-5.868033, 5.868033)
                particle.ZOffset = 0
                particle.ShapeInOut = Enum.ParticleEmitterShapeInOut.Outward
                particle.Rate = 6.978326797485352
                particle.VelocityInheritance = 0
                particle.Rotation = NumberRange.new(-360.000000, 360.000000)
                particle.ShapePartial = 1
                particle.Texture = "rbxassetid://103380620904558"
                particle.FlipbookMode = Enum.ParticleFlipbookMode.OneShot
