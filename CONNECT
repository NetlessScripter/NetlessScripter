local Players = game:GetService("Players")
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

local ssjFaceId = "rbxassetid://12876975875"
local specialFaceId = "rbxassetid://12928673086"  
local specialAnimId = "rbxassetid://12447707844"

local ssjAnimIds = {
    ["rbxassetid://11343318134"] = true,
    ["rbxassetid://11365563255"] = true,
    ["rbxassetid://12983333733"] = true,
    ["rbxassetid://13927612951"] = true,
}

local head = character:WaitForChild("Head")
local face = head:FindFirstChildWhichIsA("Decal")

if face then
    face.Texture = ssjFaceId
end

local function onAnimationPlayed(animationTrack)
    if not face then return end

    local animId = animationTrack.Animation.AnimationId

    if animId == specialAnimId then
        face.Texture = ""
        task.wait()  
        face.Texture = specialFaceId
    elseif ssjAnimIds[animId] then
        face.Texture = ""
        task.wait()  
        face.Texture = ssjFaceId
    end
end

humanoid.AnimationPlayed:Connect(onAnimationPlayed)

local pointLight = Instance.new("PointLight")
pointLight.Parent = hair
pointLight.Color = Color3.fromRGB(255, 255, 150)
pointLight.Range = 8
pointLight.Brightness = 1
pointLight.Enabled = true

local function removePlayerHair(player)
    local character = player.Character or player.CharacterAdded:Wait()
    for _, accessory in pairs(character:GetChildren()) do
        if accessory:IsA("Accessory") and accessory.AccessoryType == Enum.AccessoryType.Hair then
            accessory:Destroy()
        end
    end
end

game.Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function()
        removePlayerHair(player)
    end)
end)

