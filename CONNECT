local function PlayVideoWithSound(videoUrl, soundUrl, baseName)
    local videoFile = baseName .. ".mp4"
    if not isfile(videoFile) then
        writefile(videoFile, game:HttpGet(videoUrl))
    end

    local player = game.Players.LocalPlayer
    local playerGui = player:WaitForChild("PlayerGui")

    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "FullscreenVideoGui"
    screenGui.IgnoreGuiInset = true
    screenGui.ResetOnSpawn = false
    screenGui.ZIndexBehavior = Enum.ZIndexBehavior.Global
    screenGui.DisplayOrder = 1e6
    screenGui.Parent = playerGui

    local videoFrame = Instance.new("VideoFrame")
    videoFrame.Size = UDim2.new(1, 0, 1, 0)
    videoFrame.Position = UDim2.new(0, 0, 0, 0)
    videoFrame.BackgroundTransparency = 1
    videoFrame.Video = (getsynasset or getcustomasset)(videoFile)
    videoFrame.Looped = false
    videoFrame.ZIndex = 1e6
    videoFrame.Parent = screenGui

    local sound = GetGitSound(soundUrl, "Gokuintrosound")

    videoFrame.Ended:Connect(function()
        sound:Stop()
        screenGui:Destroy()

        local StarterGui = game:GetService("StarterGui")
local discordLink = "https://discord.gg/TqyDFYdseB"
setclipboard(discordLink)
StarterGui:SetCore("SendNotification", {
    Title = "NOTIFICATION",
    Text = "Copied Discord Link SCRIPT IS NOT BY C00LKID",
    Duration = 5
})
local nigga = Instance.new("Sound")
nigga.SoundId = "rbxassetid://3398620867"
nigga.Volume = 3
nigga.Parent = workspace
nigga:Play()
    end)

    videoFrame:Play()
    sound:Play()
end

