local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

if character:GetAttribute("Character") then
    local function onCharacterChanged()
        local attributeValue = character:GetAttribute("Character")
        local func = cH[attributeValue]
        if func then
            func()
        else
            warn("No function found for attribute value:", attributeValue)
        end
    end

    character:GetAttributeChangedSignal("Character"):Connect(onCharacterChanged)

    onCharacterChanged()
else
    warn("The attribute 'Character' does not exist on the player's character.")
end
