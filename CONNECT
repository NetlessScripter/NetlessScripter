local animationAssetId = "rbxassetid://105122779439662"

local AnimHandler = loadstring(game:HttpGet("https://raw.githubusercontent.com/ProudNamed/SuperLightning/refs/heads/main/AnimModule/MainModule"))()

local currentAnimInstance

local function GetGitSound(GithubSnd, SoundName)
    if not isfile(SoundName .. ".mp3") then
        writefile(SoundName .. ".mp3", game:HttpGet(GithubSnd))
    end
    local sound = Instance.new("Sound")
    sound.SoundId = (getcustomasset or getsynasset)(SoundName .. ".mp3")
    sound.Volume = 1
    sound.Name = SoundName
    sound.Parent = workspace
    return sound
end

local function stopAllAnimations(humanoid)
    for _, track in ipairs(humanoid:GetPlayingAnimationTracks()) do
        track:Stop()
    end
end

local function playSlowAnimation(char)
    local humanoid = char:WaitForChild("Humanoid")
    stopAllAnimations(humanoid)

    local animModel = game:GetObjects(animationAssetId)[1]
    animModel.Parent = char

    game:GetService("ContentProvider"):PreloadAsync({animModel})

    currentAnimInstance = AnimHandler.new(char, animModel)
    currentAnimInstance:Play()
    currentAnimInstance:SetSpeed(0.2)
end

local function onSoundPlayed(originalSound, index)
    local newSound = GetGitSound(githubSoundUrls[index], "GitHubSound" .. index)
    newSound:Play()
    newSound.Ended:Connect(function()
        newSound:Destroy()
    end)

    if index == 1 then

        playSlowAnimation(character)
    elseif index == 2 and currentAnimInstance then

        currentAnimInstance:SetSpeed(1)
    end
end

local function hookSound(sound)
    for i, soundId in ipairs(soundIdsToCheck) do
        if sound.SoundId == soundId then
            sound.Volume = 0
            if not sound:GetAttribute("GitHubHooked") then
                sound:SetAttribute("GitHubHooked", true)
                sound.Played:Connect(function()
                    onSoundPlayed(sound, i)
                end)
            end
            break
        end
    end
end

local function hookCharacterSounds(char)
    for _, sound in ipairs(char:GetDescendants()) do
        if sound:IsA("Sound") then
            hookSound(sound)
        end
    end
    char.DescendantAdded:Connect(function(desc)
        if desc:IsA("Sound") then
            hookSound(desc)
        end
    end)
end

local function onCharacterAdded(char)
    character = char
    hookCharacterSounds(char)
end

onCharacterAdded(character)
player.CharacterAdded:Connect(onCharacterAdded)

