local targetAnimID = "rbxassetid://10468665991" 

local function onAnimationPlayed(animationTrack)
    if animationTrack.Animation.AnimationId == targetAnimID then

        animationTrack:Stop()

_G.PlayStoredAnimation90()

        task.spawn(function()
            local TweenService = game:GetService("TweenService")
            local Camera = workspace.CurrentCamera

            local function tweenFOV(targetFOV, duration)
                local tween = TweenService:Create(Camera, TweenInfo.new(duration, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {FieldOfView = targetFOV})
                tween:Play()
                return tween
            end

            tweenFOV(60, 0.5)
            task.wait(0.5)

            tweenFOV(80, 0.3)

            task.delay(1, function()
                tweenFOV(70, 0.3)
            end)
        end)

        local soundUrl1 = "https://github.com/NetlessScripter/GOKU/blob/3ed2cc20f8208290c98b0f640d4dbd1b63f4e342/copy_C205011C-503B-4E0F-AC25-9E47BE56E236%202.mp3?raw=true"
        local soundName1 = "CustomSound"

        local function GetGitSound(GithubSnd, SoundName)
            local url = GithubSnd
            if not isfile(SoundName .. ".mp3") then
                writefile(SoundName .. ".mp3", game:HttpGet(url))
            end
            local sound = Instance.new("Sound")
            sound.SoundId = (getcustomasset or getsynasset)(SoundName .. ".mp3")
            sound.Volume = 1
            sound.Name = SoundName
            sound.Parent = workspace
            return sound
        end

        local sound1 = GetGitSound(soundUrl1, soundName1)
        sound1:Play()

        sound1:GetPropertyChangedSignal("TimeLength"):Connect(function()
            local totalTime = sound1.TimeLength
            if totalTime > 0 then
                task.delay(totalTime - 0.6, function()
                    local startVolume = sound1.Volume
                    local fadeTime = 0.6
                    local steps = 30
                    for i = 1, steps do
                        sound1.Volume = startVolume * (1 - i / steps)
                        task.wait(fadeTime / steps)
                    end
                    sound1:Stop()
                    sound1:Destroy()
                end)
            end
        end)

        local sound2 = GetGitSound("https://github.com/NetlessScripter/GOKU/blob/fe932410b649a18432468eddbb8d4bfe106e6bf5/copy_5801B72C-B51F-4D23-A425-20E3BB77A75C.mp3?raw=true", "MyCustomSound")
        sound2.Volume = 1.4
        sound2:Play()

        local TweenService = game:GetService("TweenService")
        local player = Player
        local character = Character
        local rightArm = character:WaitForChild("Right Arm") 
        local testEffect = game.ReplicatedStorage.Emotes.ClashEmit.ClashEmit:Clone()

        local handle = Instance.new("Part")
        handle.Name = "EffectHandle"
        handle.Size = Vector3.new(0.5, 0.5, 0.5)
        handle.Transparency = 1
        handle.CanCollide = false
        handle.Anchored = false
        handle.Massless = true
        handle.Parent = character

        handle.CFrame = rightArm.CFrame
        local weld = Instance.new("WeldConstraint")
        weld.Part0 = rightArm
        weld.Part1 = handle
        weld.Parent = handle

        testEffect.Parent = handle

        for _, child in ipairs(testEffect:GetChildren()) do
            if child:IsA("ParticleEmitter") then
                child.Enabled = true
                child:Emit(15)

                local sizeSeq = child.Size
                local newSizeKeypoints = {}
                for _, keypoint in ipairs(sizeSeq.Keypoints) do
                    table.insert(newSizeKeypoints, NumberSequenceKeypoint.new(keypoint.Time, keypoint.Value * 0.5, keypoint.Envelope))
                end
                child.Size = NumberSequence.new(newSizeKeypoints)
                child.Speed = NumberRange.new(child.Speed.Min * 0.5, child.Speed.Max * 0.5)
            end
        end

        local function fadeOutAndDestroy(model, duration)
            local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Linear)
            local tweens = {}

            for _, part in ipairs(model:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                    table.insert(tweens, TweenService:Create(part, tweenInfo, {Transparency = 1}))
                elseif part:IsA("Decal") or part:IsA("Texture") then
                    table.insert(tweens, TweenService:Create(part, tweenInfo, {Transparency = 1}))
                end
            end

            for _, tween in ipairs(tweens) do
                tween:Play()
            end

            delay(duration, function()
                if model and model.Parent then
                    model:Destroy()
                end
            end)
        end

        delay(0.6, function()
            if handle and handle.Parent then
                handle:Destroy()
            end

            StoredAnimation90.TimePosition = 0.9

            local player = game.Players.LocalPlayer
            local char = player.Character or player.CharacterAdded:Wait()
            local hrp = char:WaitForChild("HumanoidRootPart")

            local model = game:GetObjects("rbxassetid://115677204094535")[1]
            model.Parent = workspace

            local refPart
            for _, part in pairs(model:GetDescendants()) do
                if part:IsA("BasePart") then
                    refPart = part
                    break
                end
            end

            if refPart then
                local refCFrame = refPart.CFrame
                local targetCFrame = hrp.CFrame * CFrame.new(0, 0, -24)

                for _, part in pairs(model:GetDescendants()) do
                    if part:IsA("BasePart") then
                        local offset = refCFrame:ToObjectSpace(part.CFrame)
                        part.CFrame = targetCFrame * offset
                    end
                end
            end

            fadeOutAndDestroy(model, 1)
        end)
    end
end

