local sound = GetGitSound(
    "https://github.com/NetlessScripter/GOKU/blob/5b3ac08281bbd2939f5dc8e9219b7f51a7604662/copy_BD21A594-0B70-45EA-8757-39EEB8949364.mp3?raw=true",
