-- Blocked sound list
local blockedSounds = {
	["rbxassetid://3755636825"] = true,
	["rbxassetid://3755636438"] = true,
	["rbxassetid://10472822874"] = true,
	["swoosh-long3"] = true,
	["swoosh-long1"] = true,
	["Audio/kickswing"] = true
}

-- Cleanup previous animation track
local currentTrack = nil

task.spawn(function()
	while true do
		for _, s in ipairs(character:GetDescendants()) do
			if s:IsA("Sound") then
				if blockedSounds[s.SoundId] or blockedSounds[s.Name] then
					s:Stop()
					s.Volume = 0
				end
			end
		end
		task.wait()
	end
end)

-- Detect animations, stop old one, play replacement + sound
humanoid.AnimationPlayed:Connect(function(animationTrack)
	local animId = animationTrack.Animation.AnimationId
	local data = animationSoundMap[animId]
	if data then
		animationTrack:Stop()
		GetGitSound(data.url, data.name)

		-- Stop currently playing animation if there is one
		if currentTrack and currentTrack.IsPlaying then
			currentTrack:Stop()
		end

		local newAnim = Instance.new("Animation")
		newAnim.AnimationId = data.anim
		currentTrack = animator:LoadAnimation(newAnim)
		currentTrack:Play()
	end
end)

local targetAnimID = "rbxassetid://12447707844" -- Updated target animation ID

local function onAnimationPlayed(animationTrack)
    if animationTrack.Animation.AnimationId == targetAnimID then
        -- Stop the animation immediately when detected
       task.spawn(function()
	local rs = game:GetService("RunService")
	local ppl = game:GetService("Players")
	local cam = game:GetService("Workspace").CurrentCamera

	local plr = ppl.LocalPlayer
	local char = plr.Character or plr.CharacterAdded:Wait()
	local hum: Humanoid = char:WaitForChild("Humanoid")
	local RootPart: Part = char:WaitForChild("HumanoidRootPart")

	hum.Died:Connect(function()
		char = plr.Character or plr.CharacterAdded:Wait()
		hum = char:WaitForChild("Humanoid")
		RootPart = char:WaitForChild("HumanoidRootPart")
	end)

	task.delay(15, function()
		rs:BindToRenderStep("FollowHead", Enum.RenderPriority.Camera.Value + 1, function(DeltaTime: number)
			if not char or (hum:GetState() == Enum.HumanoidStateType.Dead) then
				return
			end

			local Head: Part = char:WaitForChild("Head")
			local ObjectSpace = RootPart.CFrame:ToObjectSpace(Head.CFrame)
			hum.CameraOffset = hum.CameraOffset:Lerp(ObjectSpace.Position - Vector3.new(0, 1.5, 0), DeltaTime * 10)

			-- Make player face the direction of the camera (horizontal only)
			local camLook = cam.CFrame.LookVector
			camLook = Vector3.new(camLook.X, 0, camLook.Z).Unit
			local lookCFrame = CFrame.new(RootPart.Position, RootPart.Position + camLook)
			RootPart.CFrame = CFrame.new(RootPart.Position) * CFrame.Angles(0, lookCFrame.LookVector:Angle(Vector3.new(0, 0, -1)), 0)
		end)
	end)
end)

 animationTrack:Stop()

loadstring(game:HttpGet("https://raw.githubusercontent.com/NetlessScripter/Vegito-/refs/heads/main/TEST"))()


        -- You can add additional functionality here if needed for the animation
    end
end

-- Connect animation played event
game.Players.LocalPlayer.Character:WaitForChild("Humanoid").AnimationPlayed:Connect(onAnimationPlayed)

