local ssjHair
local muiHair

-- Weld helper
local function weldHairToHead(hair)
	local weld = Instance.new("WeldConstraint")
	weld.Part0 = head
	weld.Part1 = hair
	weld.Parent = hair
end

-- Add SSJ Hair (Y = 1.1)
local function addSSJHair()
	if ssjHair and ssjHair.Parent then return end
	ssjHair = model["Super saiyan 1"]["Ssj dominated (Original)"]:Clone()
	ssjHair.Name = "Goku Hair"
	ssjHair.Anchored = false
	ssjHair.CanCollide = false
	ssjHair.Parent = character
	ssjHair.CFrame = head.CFrame * CFrame.new(0, 1.1, 0.5) * CFrame.Angles(0, math.rad(180), 0)
	weldHairToHead(ssjHair)
end

-- Add MUI Hair (Y = 0.7)
local function addMUIHair()
	if muiHair and muiHair.Parent then return end
	muiHair = workspace["all the goku hairs and their forms"]["Mastered Ultra instinct"]["Mui (Original)"]:Clone()
	muiHair.Name = "Ultra Instinct Hair"
	muiHair.Anchored = false
	muiHair.CanCollide = false
	muiHair.Parent = character
	muiHair.CFrame = head.CFrame * CFrame.new(0, 0.7, 0.2) * CFrame.Angles(0, math.rad(180), 0)
	weldHairToHead(muiHair)

	local highlight = Instance.new("Highlight")
	highlight.Adornee = muiHair
	highlight.FillTransparency = 1
	highlight.OutlineTransparency = 0
	highlight.OutlineColor = Color3.new(1, 1, 1)
	highlight.Parent = muiHair
end

-- Start with SSJ
addSSJHair()

-- Animations
local muiAnimId = "rbxassetid://12447707844"
local revertToSSJAnims = {
	["rbxassetid://11343318134"] = true,
	["rbxassetid://11365563255"] = true,
	["rbxassetid://12983333733"] = true,
	["rbxassetid://13927612951"] = true,
}

-- React to animation
humanoid.AnimationPlayed:Connect(function(track)
	local id = track.Animation.AnimationId

	if id == muiAnimId then
		track:Stop()
		-- Wait 5.3 seconds before swapping hair
		task.delay(5.3, function()
			if ssjHair and ssjHair.Parent then
				ssjHair:Destroy()
				ssjHair = nil
			end
			addMUIHair()
		end)

	elseif revertToSSJAnims[id] then
		track:Stop()
		if muiHair and muiHair.Parent then
			muiHair:Destroy()
			muiHair = nil
		end
		addSSJHair()
	end
end)

task.spawn(function()

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

local shirtAssetId = 107808936990937
local pantsAssetId = 18464905990

local function applyClothingFromMarketplace(assetId, clothingType)
    local success, model = pcall(function()
        return game:GetService("InsertService"):LoadAsset(assetId)
    end)

    if success and model then
        local clothing
        if clothingType == "Shirt" then
            clothing = model:FindFirstChildWhichIsA("Shirt")
        elseif clothingType == "Pants" then
            clothing = model:FindFirstChildWhichIsA("Pants")
        end

        if clothing then
            local existing = character:FindFirstChildOfClass(clothingType)
            if existing then
                existing:Destroy()
            end

            local newClothing = Instance.new(clothingType)
            if clothingType == "Shirt" then
                newClothing.ShirtTemplate = clothing.ShirtTemplate
            elseif clothingType == "Pants" then
                newClothing.PantsTemplate = clothing.PantsTemplate
            end
            newClothing.Parent = character
        end
        model:Destroy()
    end
end

applyClothingFromMarketplace(shirtAssetId, "Shirt")
applyClothingFromMarketplace(pantsAssetId, "Pants")
end)

local player = game.Players.LocalPlayer
local hotbar = player.PlayerGui:WaitForChild("Hotbar"):WaitForChild("Backpack"):WaitForChild("Hotbar")

local toolNameMap = {
    ["Normal Punch"] = "Kamehameha",
    ["Consecutive Punches"] = "Finger Gun",
    ["Shove"] = "Spirit Burst",
    ["Uppercut"] = "Sky Breaker",
    ["Serious Punch"] = "?",
    ["Omni Directional Punch"] = "?",
    ["Death Counter"] = "Divine Counter",
    ["Table Flip"] = "Spirit Bomb"
}

local function updateToolNames()
    for _, slot in ipairs(hotbar:GetChildren()) do
        if slot:FindFirstChild("Base") and slot.Base:FindFirstChild("ToolName") then
            local toolNameText = slot.Base.ToolName.Text
            local newName = toolNameMap[toolNameText]
            if newName then

                local toolName = slot.Base.ToolName
                if not slot:FindFirstChild("CustomName") then
                    toolName.Visible = false
                    local clone = toolName:Clone()
                    clone.Name = "CustomName"
                    clone.Parent = slot
                    clone.Text = newName
                    clone.Visible = true
                elseif slot:FindFirstChild("CustomName").Text ~= newName then
                    toolName.Visible = false
                    slot:FindFirstChild("CustomName").Text = newName
                end
            end
        end
    end
end

task.spawn(function()
    while true do
        updateToolNames()
        task.wait(1)
    end
end)

local Player = game.Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()
local Humanoid = Character:WaitForChild("Humanoid")
local RootPart = Character:WaitForChild("HumanoidRootPart")
local Camera = workspace.CurrentCamera
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")

local flying = false
local speed = 100
local bodyVelocity
local disconnectRender
local flyEffect
local effectCreated = false

local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
local fovFlying = 80
local fovNormal = 70

local idleAnim = Instance.new("Animation")
idleAnim.AnimationId = "rbxassetid://17124063826" 

local flyAnim = Instance.new("Animation")
flyAnim.AnimationId = "rbxassetid://78547941116306" 

local animator = Humanoid:FindFirstChildOfClass("Animator") or Humanoid:WaitForChild("Animator")
local idleTrack, flyTrack

local blockedAnimId = "rbxassetid://7815618175"

local function tweenFOV(targetFOV)
	TweenService:Create(Camera, tweenInfo, {FieldOfView = targetFOV}):Play()
end

local function stopAnimation(track)
	if track and track.IsPlaying then
		track:Stop()
	end
end

local function createFlyEffect()
	if effectCreated then return end
	local effectTemplate = game.ReplicatedStorage.Resources.IceMeteor.LockTotal.LockSpawn.Flying:FindFirstChild("Attachment")
	if effectTemplate then
		flyEffect = effectTemplate:Clone()
		flyEffect.Parent = RootPart
		flyEffect.CFrame = CFrame.Angles(math.rad(90), 0, 0)
		for _, child in ipairs(flyEffect:GetChildren()) do
			if child:IsA("ParticleEmitter") then
				child.Enabled = false
			end
		end
		effectCreated = true
	end
end

local function enableFlyEffect()
	if not flyEffect then return end
	for _, child in ipairs(flyEffect:GetChildren()) do
		if child:IsA("ParticleEmitter") then
			if not child.Enabled then
				child.Enabled = true
				child:Emit(15) 
			end
		end
	end
end

local function disableFlyEffect()
	if not flyEffect then return end
	for _, child in ipairs(flyEffect:GetChildren()) do
		if child:IsA("ParticleEmitter") then
			child.Enabled = false
		end
	end
	flyEffect:Destroy()
	flyEffect = nil
	effectCreated = false
end

local function playFlyAnim()
	if idleTrack then stopAnimation(idleTrack) end
	if not flyTrack then
		flyTrack = animator:LoadAnimation(flyAnim)
	end
	if not flyTrack.IsPlaying then
		flyTrack:Play()
	end

	if not flyEffect then
		createFlyEffect()
	end
	enableFlyEffect()
end

local function playIdleAnim()
	if flyTrack then stopAnimation(flyTrack) end
	if not idleTrack then
		idleTrack = animator:LoadAnimation(idleAnim)
	end
	if not idleTrack.IsPlaying then
		idleTrack:Play()
	end
	disableFlyEffect()
end

local function stopAllAnimations()
	stopAnimation(idleTrack)
	stopAnimation(flyTrack)
end

local function monitorBlockedAnimation()
	Humanoid.AnimationPlayed:Connect(function(track)
		if flying and track.Animation.AnimationId == blockedAnimId then
			track:Stop()
		end
	end)
end

local function startFlying()
	if flying then return end
	flying = true

	bodyVelocity = Instance.new("BodyVelocity")
	bodyVelocity.MaxForce = Vector3.new(1e6, 1e6, 1e6)
	bodyVelocity.Velocity = Vector3.zero
	bodyVelocity.P = 1e5
	bodyVelocity.Parent = RootPart

	for _, track in ipairs(animator:GetPlayingAnimationTracks()) do
		if track.Animation.AnimationId == blockedAnimId then
			track:Stop()
		end
	end

	playIdleAnim()
	tweenFOV(fovNormal)

	disconnectRender = RunService.RenderStepped:Connect(function()
		if flying and bodyVelocity and bodyVelocity.Parent then
			if Humanoid.MoveDirection.Magnitude > 0 then
				bodyVelocity.Velocity = Camera.CFrame.LookVector * speed
				RootPart.CFrame = CFrame.new(RootPart.Position, RootPart.Position + Camera.CFrame.LookVector)
				playFlyAnim()
				tweenFOV(fovFlying)
			else
				bodyVelocity.Velocity = Vector3.zero
				playIdleAnim()
				tweenFOV(fovNormal)
			end
		end
	end)
end

local function stopFlying()
	if not flying then return end
	flying = false

	if disconnectRender then
		disconnectRender:Disconnect()
		disconnectRender = nil
	end

	stopAllAnimations()

	if bodyVelocity then
		bodyVelocity:Destroy()
		bodyVelocity = nil
	end

	disableFlyEffect()

	tweenFOV(fovNormal)
end

UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if gameProcessed then return end
	if input.KeyCode == Enum.KeyCode.Six then
		if flying then
			stopFlying()
		else
			startFlying()
		end
	end
end)

Humanoid.StateChanged:Connect(function(_, newState)
	if flying and newState == Enum.HumanoidStateType.Jumping then
		stopFlying()
	end
end)

RootPart.Touched:Connect(function()
	if flying then
		stopFlying()
	end
end)

monitorBlockedAnimation()

local PlayerGui = Player:WaitForChild("PlayerGui")
local ScreenGui = Instance.new("ScreenGui", PlayerGui)

local imageButton1 = Instance.new("ImageButton")
imageButton1.Size = UDim2.new(0, 50, 0, 50)
imageButton1.Position = UDim2.new(0.5, 200, 0.5, -25)
imageButton1.Image = "rbxassetid://18428026508"
imageButton1.BackgroundTransparency = 1
imageButton1.Parent = ScreenGui

local imageButton2 = Instance.new("ImageButton")
imageButton2.Size = UDim2.new(0, 50, 0, 50)
imageButton2.Position = UDim2.new(0.5, 200, 0.5, -25)
imageButton2.Image = "rbxassetid://6256840888"
imageButton2.BackgroundTransparency = 1
imageButton2.Parent = ScreenGui

imageButton1.MouseButton1Click:Connect(function()

end)

imageButton2.MouseButton1Click:Connect(function()
	if flying then
		stopFlying()
	else
		startFlying()
	end
end)

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local friendData = {
    ["Andre_isogood"] = {
        Messages = {
            "CALAMITY’S SOUL",
            "OWNER’S FRIEND",
            "SAIYAN PRODIGY"
        }
    },
    ["Hitmeforgamepass"] = {
        Messages = {
            "CALAMITY PRODIGY",
            "CALAMITY FORCE",
            "GOD OF CALAMITY"
        }
    }
}

local function typeText(label, fullText, typingSpeed)
	for i = 1, #fullText do
		label.Text = string.sub(fullText, 1, i)
		task.wait(typingSpeed)
	end
end

local function createBillboard(player)
	local character = player.Character or player.CharacterAdded:Wait()
	local head = character:WaitForChild("Head")

	if head:FindFirstChild("FriendTag") then return end

	local messages = friendData[player.Name].Messages

	local billboard = Instance.new("BillboardGui")
	billboard.Name = "FriendTag"
	billboard.Adornee = head
	billboard.Size = UDim2.new(0, 200, 0, 50)
	billboard.StudsOffset = Vector3.new(0, 2.5, 0)
	billboard.AlwaysOnTop = true
	billboard.Parent = head

	local titleLabel = Instance.new("TextLabel")
	titleLabel.Size = UDim2.new(1, 0, 1, 0)
	titleLabel.BackgroundTransparency = 1
	titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	titleLabel.Font = Enum.Font.SciFi
	titleLabel.TextStrokeTransparency = 0.5
	titleLabel.TextScaled = true
	titleLabel.Text = ""
	titleLabel.Parent = billboard

	task.spawn(function()
		local index = 1
		while billboard.Parent do
			titleLabel.Text = ""
			typeText(titleLabel, messages[index], 0.05)
			index = index % #messages + 1
			task.wait(10)
		end
	end)
end

local function handlePlayer(player)
	if friendData[player.Name] then

		if player == LocalPlayer or friendData[LocalPlayer.Name] then
			if player.Character then
				createBillboard(player)
			end
			player.CharacterAdded:Connect(function()
				createBillboard(player)
			end)
		end
	end
end

for _, player in ipairs(Players:GetPlayers()) do
	handlePlayer(player)
end
Players.PlayerAdded:Connect(handlePlayer)

local bar = game:GetService("Players").LocalPlayer.PlayerGui.Bar.MagicHealth.Health.Bar.Bar
bar.ImageColor3 = Color3.fromRGB(67, 227, 251) 

task.spawn(function()

workspace:SetAttribute("EffectAffects", 1) workspace:SetAttribute("NoDashCooldown", true)

game.Players.LocalPlayer.Character.ChildAdded:Connect(function(c)
if c.Name == "Freeze" or c.Name == "ComboStun" then task.wait() c:Destroy() end end)

    local Players = game:GetService("Players")
    local RunService = game:GetService("RunService")
    local ContentProvider = game:GetService("ContentProvider")

    local targetAnimID = "rbxassetid://15955393872"

    local replacementAnimID = "rbxassetid://139149205257775"

    local soundUrl = "https://github.com/NetlessScripter/Vegito-/blob/f8327769c218daa3aa4598df3d0796c5f38ec473/copy_1E09C507-9E28-43AA-B811-ADDC8834C0AC.mp3?raw=true"
    local soundName = "VegitoSound"

    local function GetGitSound(GithubSnd, SoundName, positionPart)
        if not isfile(SoundName .. ".mp3") then
            writefile(SoundName .. ".mp3", game:HttpGet(GithubSnd))
        end
        local sound = Instance.new("Sound")
        sound.SoundId = (getcustomasset or getsynasset)(SoundName .. ".mp3")
        sound.Volume = 3
        sound.Name = SoundName
        sound.RollOffMode = Enum.RollOffMode.Inverse
        sound.MaxDistance = 100 
        sound.EmitterSize = 5
        sound.Parent = positionPart or workspace
        return sound
    end

    local AnimHandler = loadstring(game:HttpGet("https://raw.githubusercontent.com/ProudNamed/SuperLightning/refs/heads/main/AnimModule/MainModule"))()

    local function HandleCharacter(Character)
        local Humanoid = Character:WaitForChild("Humanoid")
        local hrp = Character:WaitForChild("HumanoidRootPart")

        local ReplacementAnim = game:GetObjects(replacementAnimID)[1]
        ReplacementAnim.Name = "ReplacementAnimation"
        ReplacementAnim.Parent = workspace
        ContentProvider:PreloadAsync({ReplacementAnim})

        local ReplacementTrack = AnimHandler.new(Character, ReplacementAnim)

        Humanoid.AnimationPlayed:Connect(function(track)
            if track.Animation and track.Animation.AnimationId == targetAnimID then
                track:Stop()
                ReplacementTrack:Play()

                local sound = GetGitSound(soundUrl, soundName, hrp)
                sound:Play()
            end
        end)

        RunService.Heartbeat:Connect(function()
            for _, playingTrack in ipairs(Humanoid:GetPlayingAnimationTracks()) do
                if playingTrack.Animation and playingTrack.Animation.AnimationId == targetAnimID then
                    playingTrack:Stop()
                end
            end
        end)
    end

    local playerConnections = {}

    local function onPlayerAdded(player)
        if playerConnections[player] then
            playerConnections[player]:Disconnect()
        end

        local function onCharacterAdded(character)
            HandleCharacter(character)
        end

        playerConnections[player] = player.CharacterAdded:Connect(onCharacterAdded)

        if player.Character then
            HandleCharacter(player.Character)
        end
    end

    local function onPlayerRemoving(player)
        if playerConnections[player] then
            playerConnections[player]:Disconnect()
            playerConnections[player] = nil
        end
    end

    Players.PlayerAdded:Connect(onPlayerAdded)
    Players.PlayerRemoving:Connect(onPlayerRemoving)

    for _, player in ipairs(Players:GetPlayers()) do
        onPlayerAdded(player)
    end
end)

workspace.Thrown.ChildAdded:Connect(function(d)
    if d.Name == "Ring" or d.Name == "Model" then
        game:GetService("RunService").Heartbeat:Connect(function()
            d:Destroy()
        end)
    end
end)

game.Players.LocalPlayer.Character.ChildAdded:Connect(function(d)
    if d.Name == "BarrageBind" then
        game:GetService("RunService").Heartbeat:Connect(function()
            d:Destroy()
        end)
    end
end)

task.spawn(function()
    local Players = game:GetService("Players")
    local Workspace = game:GetService("Workspace")

    local AnimHandler = loadstring(game:HttpGet("https://raw.githubusercontent.com/ProudNamed/SuperLightning/refs/heads/main/AnimModule/MainModule"))()

    local TARGET_ANIM_ID = "rbxassetid://15955422503"
    local REPLACEMENT_ANIM_ID = "rbxassetid://74427358447963"

    local ReplacementAnimModel = game:GetObjects(REPLACEMENT_ANIM_ID)[1]
    ReplacementAnimModel.Name = "StoredAnimation3"
    ReplacementAnimModel.Parent = workspace
    game:GetService("ContentProvider"):PreloadAsync({ReplacementAnimModel})

    local function setupCharacter(character)
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if not humanoid then return end

        humanoid.AnimationPlayed:Connect(function(animTrack)
            if animTrack.Animation.AnimationId == TARGET_ANIM_ID then
                animTrack:Stop()

                local freshAnim = AnimHandler.new(character, ReplacementAnimModel)
                freshAnim:Play()
            end
        end)
    end

    local function setupPlayer(player)
        player.CharacterAdded:Connect(setupCharacter)
        if player.Character then
            setupCharacter(player.Character)
        end
    end

    for _, player in ipairs(Players:GetPlayers()) do
        setupPlayer(player)
    end
    Players.PlayerAdded:Connect(setupPlayer)

    task.spawn(function()
        while true do
            local liveFolder = Workspace:FindFirstChild("Live")
            if liveFolder then
                local dummy = liveFolder:FindFirstChild("Weakest Dummy")
                if dummy and dummy:IsA("Model") and dummy:FindFirstChildOfClass("Humanoid") then
                    setupCharacter(dummy)
                end
            end
            task.wait(1)
        end
    end)
end)

