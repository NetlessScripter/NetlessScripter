local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local workspace = game:GetService("Workspace")
local replicatedStorage = game:GetService("ReplicatedStorage")
local players = game:GetService("Players")

local function getPlayerInFront()
    local direction = humanoidRootPart.CFrame.LookVector * 5  
    local raycastParams = RaycastParams.new()
    raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
    raycastParams.FilterDescendantsInstances = {character}  

    local result = workspace:Raycast(humanoidRootPart.Position, direction, raycastParams)
    if result and result.Instance then
        local hitCharacter = result.Instance.Parent
        if hitCharacter and hitCharacter:FindFirstChild("Humanoid") then
            return hitCharacter
        end
    end
    return nil
end

local function applyDragonComboEffect(targetCharacter)
    local effectTemplate = replicatedStorage.Emotes.VFX.RealAssets["Dragon Combo"].Punch.Attachment
    if not effectTemplate then return end

    local effectClone = effectTemplate:Clone()
    effectClone.Parent = targetCharacter:FindFirstChild("HumanoidRootPart") or targetCharacter

    for _, emitter in ipairs(effectClone:GetChildren()) do
        if emitter:IsA("ParticleEmitter") then
            emitter:Emit(15) 
        end
    end
end

local function applyDropkickEffect()

    local part = Instance.new("Part")
    part.Parent = workspace
    part.Transparency = 1
    part.CFrame = humanoidRootPart.CFrame * CFrame.new(0, 0, -5)
    part.Anchored = true
    part.CanCollide = false
    game:GetService("Debris"):AddItem(part, 2)

    local v9 = replicatedStorage.Resources.KJEffects.DropkickExtra.firstHit.Attachment.Wind:Clone()
    local v8 = replicatedStorage.Resources.KJEffects.DropkickExtra.firstHit:GetChildren()[3]:GetChildren()[2].wind:Clone()
    v8.Parent = part
    v9:Emit(10)
    v9.Parent = part
    v9:Emit(10)
    v9.Brightness = 1
end

local function playImpactSound(targetCharacter, soundId)
    local sound = Instance.new("Sound")
    sound.SoundId = soundId
    sound.Volume = 0
    sound.Parent = targetCharacter:FindFirstChild("HumanoidRootPart") or targetCharacter
    sound:Play()
    game:GetService("Debris"):AddItem(sound, 2)  
end

local animationIDs = {
    ["rbxassetid://10469493270"] = "M1",
    ["rbxassetid://10469630950"] = "M2",
    ["rbxassetid://10469639222"] = "M3",
    ["rbxassetid://10469643643"] = "M4"
}
local animationSounds = {
    M1 = "rbxassetid://13064223399",
    M2 = "rbxassetid://13064223291",
    M3 = "rbxassetid://13064223483",
    M4 = "rbxassetid://13064442279"
}

local humanoid = character:WaitForChild("Humanoid")
humanoid.AnimationPlayed:Connect(function(animationTrack)
    local animId = animationTrack.Animation.AnimationId
    if animationIDs[animId] then
        local target = getPlayerInFront()
        if target then
            applyDragonComboEffect(target)  
            playImpactSound(target, animationSounds[animationIDs[animId]]) 
        end
        applyDropkickEffect() 
    end
end)

