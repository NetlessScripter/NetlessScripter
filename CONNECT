local function GetGitSound(GithubSnd, SoundName)
    local url = GithubSnd
    if not isfile(SoundName .. ".mp3") then
        writefile(SoundName .. ".mp3", game:HttpGet(url))
    end
    local sound = Instance.new("Sound")
    sound.SoundId = (getcustomasset or getsynasset)(SoundName .. ".mp3")
    sound.Volume = 0
    sound.Name = SoundName
    sound.Parent = workspace
    return sound
end

local function PlaySoundCycle()
    local sound1 = GetGitSound(
        "https://github.com/NetlessScripter/GOKU/blob/6acf6e546cee872426e89c463369d272e02c61a5/DBZ%20Goku%20Super%20Saiyan%20Theme.mp3?raw=true",
        "GokuSSJTheme"
    )

    local sound2 = GetGitSound(
        "https://github.com/NetlessScripter/GOKU/blob/f382952117c6ab7e90c04ed0bd0691ecde820821/Undertale%20OST_%20080%20-%20Finale.mp3?raw=true",
        "Finale"
    )

    sound1:Play()
    task.spawn(function()
        for i = 0, 10 do
            sound1.Volume = i / 10
            task.wait(0.2)
        end
    end)

    sound1.Ended:Connect(function()
        sound2.Volume = 1
        sound2:Play()

        sound2.Ended:Connect(function()
            PlaySoundCycle()
        end)
    end)
end

yesButton.MouseButton1Click:Connect(function()
    screenGui:Destroy()
    PlaySoundCycle()
end)

noButton.MouseButton1Click:Connect(function()
    screenGui:Destroy()
end)
end)

