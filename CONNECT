local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local lplr = Players.LocalPlayer
local character = lplr.Character or lplr.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local animator = humanoid:FindFirstChildOfClass("Animator") or Instance.new("Animator", humanoid)

local ultraInstinctAnimations = {
    "rbxassetid://133094662049155",
    "rbxassetid://109088632860488",
    "rbxassetid://134711731729986",
    "rbxassetid://78339272602733",
    "rbxassetid://92546791251633",
    "rbxassetid://76963965406296",
    "rbxassetid://128188725134114",
    "rbxassetid://127015697036075"
}

-- Function to get and play GitHub sound
local function GetGitSound(GithubSnd, SoundName)
	local url = GithubSnd
	if not isfile(SoundName .. ".mp3") then
		writefile(SoundName .. ".mp3", game:HttpGet(url))
	end
	local sound = Instance.new("Sound")
	sound.SoundId = (getcustomasset or getsynasset)(SoundName .. ".mp3")
	sound.Volume = 3
	sound.Parent = workspace
	return sound
end

-- Skibidi Tech Sound URLs
local dodgeSounds = {
    {
        url = "https://github.com/NetlessScripter/Vegeta/blob/8eda2b32c4f1b22f5e22c2243008523448bacb6c/copy_32992566-D00B-4561-826A-F80FA843E766.mp3?raw=true",
        name = "Skibidi Tech 1"
    },
    {
        url = "https://github.com/NetlessScripter/GOKU/blob/dbe43657340ebbc6df8c47e8efa8cd49c1e62871/copy_D3E7C9B0-2ACD-4F3E-9AB0-B2FE634BD986.mp3?raw=true",
        name = "Skibidi Tech 2"
    },
    {
        url = "https://github.com/NetlessScripter/GOKU/blob/3b2984c8c6e8d473045061dbe4dbe9a5a8301b87/copy_19543B24-E936-40B1-8969-EF7F7E6DBC54.mp3?raw=true",
        name = "Skibidi Tech 3"
    }
}

character:SetAttribute("Blocking", false)
character:SetAttribute("BlockReact", 0)

local previousBlockReact = character:GetAttribute("BlockReact")
local playBlock = false

local function playDodgeAnimation()
    local anim = Instance.new("Animation")
    anim.AnimationId = ultraInstinctAnimations[math.random(1, #ultraInstinctAnimations)]

    for _, track in animator:GetPlayingAnimationTracks() do
        if table.find(ultraInstinctAnimations, track.Animation.AnimationId) or track.Animation.AnimationId == "rbxassetid://10470389827" then
            track:Stop()
        end
    end

    local loadedTrack = animator:LoadAnimation(anim)
    loadedTrack.Priority = Enum.AnimationPriority.Action4
    loadedTrack:Play()
    loadedTrack:AdjustSpeed(Random.new():NextNumber(1, 1.5))

    -- Play Skibidi Tech sound
    local pick = dodgeSounds[math.random(1, #dodgeSounds)]
    local sound = GetGitSound(pick.url, pick.name)
    sound:Play()
    sound.Ended:Connect(function()
        sound:Destroy()
    end)

    -- VFX
    local rootPart = character:WaitForChild("HumanoidRootPart")
    local vfx = ReplicatedStorage.Emotes.VFX.VfxMods.Evolved.vfx.uhm.Attachment:Clone()
    vfx.Position = Vector3.new(0, -3, 0)
    vfx.Parent = rootPart

    for _, p in ipairs(vfx:GetChildren()) do
        if p:IsA("ParticleEmitter") then
            p:Emit(15)
            p.Enabled = false
        end
    end
end

local function instinct()
    local blocking = character:GetAttribute("Blocking")
    local blockReact = character:GetAttribute("BlockReact")

    if not blockReact then return end

    if blocking then
        if math.abs(math.abs(blockReact) - math.abs(previousBlockReact)) > 1 then
            playDodgeAnimation()
        elseif math.abs(blockReact) < 1 and not playBlock then
            local blockAnim = Instance.new("Animation")
            blockAnim.AnimationId = "rbxassetid://10470389827"
            local loopTrack = animator:LoadAnimation(blockAnim)
            loopTrack.Looped = true
            loopTrack:Play()
            playBlock = true
        end
    end

    previousBlockReact = blockReact
end

character:GetAttributeChangedSignal("Blocking"):Connect(function()
    if not character:GetAttribute("Blocking") then
        for _, track in animator:GetPlayingAnimationTracks() do
            if table.find(ultraInstinctAnimations, track.Animation.AnimationId) or track.Animation.AnimationId == "rbxassetid://10470389827" then
                track:Stop()
                playBlock = false
            end
        end
    end
end)

character:GetAttributeChangedSignal("BlockReact"):Connect(instinct)


